\begin_layout Plain Layout
Find 
\begin_inset Formula $\left(\alpha+1\right)$
\end_inset

 received vectors that span a subspace of dimension 
\begin_inset Formula $h$
\end_inset

.
 This implies that the 
\begin_inset Formula $\alpha$
\end_inset

 links from the middle layer carry 
\begin_inset Formula $\alpha$
\end_inset

 vectors which span a subspace of 
\begin_inset Formula $\ensuremath{\mathbb{F}}_{q_{\mathrm{s}}}^{h}$
\end_inset

 whose dimension is at least 
\begin_inset Formula $\left(h-1\right)$
\end_inset

, with 
\begin_inset Formula $q_{\mathrm{s}}=q^{t}$
\end_inset

.
\end_layout

\end_inset


\end_layout

\begin_layout Standard
For 
\begin_inset Formula $3\leq\alpha<h$
\end_inset

: all 
\begin_inset Formula $\alpha$
\end_inset

 links must be distinct 
\begin_inset Formula $\Rightarrow r\leq\left[\begin{array}{c}
\alpha\\
1
\end{array}\right]_{q_{\mathrm{s}}}\Rightarrow r\leq$
\end_inset


\end_layout

\begin_layout Standard
For 
\begin_inset Formula $\alpha\geq h\geq3$
\end_inset

: to achieve 
\begin_inset Formula $(h-1)$
\end_inset

-subspaces of 
\begin_inset Formula $\ensuremath{\mathbb{F}}_{q_{\mathrm{s}}}^{h}$
\end_inset

, no 
\begin_inset Formula $\alpha$
\end_inset

 links will contain a vector which is contained in the same 
\begin_inset Formula $(h-2)$
\end_inset

-subspace.
\end_layout

\begin_layout Standard
Hence,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
r_{max,scalar}\leq\left(\alpha-1\right)\left[\begin{array}{c}
\alpha\\
h-2
\end{array}\right]_{q_{\mathrm{s}}}\Rightarrow r_{max,scalar}\in\mathcal{O}\left(q_{\mathrm{s}}^{\left(\alpha-h+2\right)\left(h-2\right)t^{2}}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Calculate Gap 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
 & r_{max,scalar} & =r_{min,vector}\\
\Leftrightarrow & q_{\mathrm{s}}^{\left(\alpha-h+2\right)\left(h-2\right)t^{2}} & =q^{\frac{h-\alpha-1}{1-\alpha}t^{2}+\mathcal{O}(t)}\\
\Leftrightarrow & q_{\mathrm{s}} & ^{=}q^{\frac{\alpha-h+1}{\left(\alpha-1\right)\left(\alpha-h+2\right)\left(h-2\right)}t^{2}+\mathcal{O}(t)}\\
\Rightarrow & g & =q_{\mathrm{s}}-q_{v}=q^{\frac{\alpha-h+1}{\left(\alpha-1\right)\left(\alpha-h+2\right)\left(h-2\right)}t^{2}+\mathcal{O}(t)}\Square
\end{eqnarray*}

\end_inset


\end_layout

